            <kind>1</kind>
            <string>/// STATE: MOVE
if (state == "move") {
   image_speed = 0;
   
   if (path != noone) {
      path_delete(path);
      path = noone;
   }
   if (waypoint != noone) {
      with (waypoint) { instance_destroy(); }
      waypoint = noone;
   }
 
